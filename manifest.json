{
	"manifest_version": 3,
	"name": "Bookmark Search",
	"version": "0.1.0",
	"author": "Spectra",
	"description": "Bookmark utilities like searching",
	"icons": {
		"32": "icons/icon_512.png",
		"128": "icons/icon_512.png"
	},
	"action": {
		"default_title": "Bookmark Search",
		"default_popup": "src/popup/popup.html",
		"default_icon": {
			"32": "icons/icon_512.png",
			"128": "icons/icon_512.png"
		}
	},
	"permissions": [
		"bookmarks",
		"tabs",
		"favicon"
	]
}
